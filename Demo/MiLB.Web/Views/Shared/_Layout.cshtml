<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mascots of MiLB - @ViewBag.Title</title>

    @*@Styles.Render("~/Content/css")*@

    <link href="~/Content/normalize.css" rel="stylesheet" />
    <link href="~/Content/validation.css" rel="stylesheet" />
    <link href="~/Content/screen.css" rel="stylesheet" />
</head>
<body>
    <script>
        function inline() {
            console.log('i am inline');
            loaded();
        }
    </script>
    <script async type="text/javascript" onload="inline()" src="/Home/Script"></script>

    <div id="container">
        <header>
            <div id="header_center">
                <div class="line_logo">
                    <div class="logo"></div>
                </div>
            </div>
        </header>
        <div id="content">
            <div class="content">
                <div class="content">
                    @RenderBody()
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="footer_center">
            <div class="footer_ball"><div class="ball"></div></div>
            <div class="footer_text">
                Full Stack Web Performance © @DateTime.Now.Year. All Rights Reserved.
            </div>
        </div>
    </footer>
</body>

</html>
